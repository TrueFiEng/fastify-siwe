{
  "name": "fastify-siwe",
  "version": "0.1.0",
  "description": "",
  "main": "dist/cjs/src/siwe.js",
  "module": "dist/esm/src/siwe.js",
  "types": "dist/esm/src/siwe.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/src/siwe.js",
      "import": "./dist/esm/src/siwe.js",
      "default": "./dist/esm/src/siwe.js",
      "types": "./dist/esm/src/siwe.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@fastify/session": "^8.1.0",
    "@prisma/client": "^3.12.0",
    "fastify": "^3.28.0",
    "fastify-cookie": "^5.6.1",
    "fastify-plugin": "^3.0.1",
    "siwe": "^1.1.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@types/node": "^17.0.10",
    "@types/react": "17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "7.19.0",
    "prettier": "^2.1.2",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc --module es2020 --target es2017 --outDir dist/esm",
    "build:cjs": "tsc --outDir dist/cjs",
    "test": "true",
    "lint": "pnpm lint:prettier --check && pnpm lint:eslint",
    "lint:fix": "pnpm lint:prettier --write && pnpm lint:eslint --fix",
    "lint:eslint": "eslint './{src,test}/**/*.{ts,tsx}'",
    "lint:prettier": "pnpm prettier './{src,test}/**/*.{ts,tsx}'",
    "version": "pnpm changeset version && pnpm install",
    "release": "pnpm build && pnpm changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrueFiEng/fastify-siwe.git"
  },
  "author": "mj426382",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TrueFiEng/fastify-siwe/issues"
  },
  "homepage": "https://github.com/TrueFiEng/fastify-siwe#readme"
}
